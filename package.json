{
  "name": "api-node-mysql",
  "version": "1.0.0",
  "description": "api restful node mysql",
  "main": "app.js",
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/bin/www.js",
    "server:prod": "node ./bundle/bin/www.js",
    "dev": "NODE_ENV=development nodemon --exec npm-run-all server",
    "build": "babel ./src --out-dir bundle",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "clean": "rimraf bundle",
    "test": "mocha test/test.js --reporter mocha-junit-reporter --reporter-options mochaFile=test-results.xml --exit || true",
    "test2": "NODE_ENV=test mocha test/test.js --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pulidovpe/api-node-mysql.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pulidovpe/api-node-mysql/issues"
  },
  "homepage": "https://github.com/pulidovpe/api-node-mysql#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "core-js": "^3.45.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.14.3",
    "npm-run-all": "^4.1.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "rimraf": "^5.0.5",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/node": "^7.28.0",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "fakerator": "^0.3.6",
    "mocha": "^11.7.1",
    "mocha-junit-reporter": "^2.2.1",
    "nodemon": "^3.1.10",
    "should": "^13.2.3",
    "supertest": "^7.1.3"
  }
}
